{
  "version": "1.0",
  "app_info":{
    "app_name": "account_manager",
    "app_organization": "Nginx Inc."
  },
  "accounts_api": {
    "log_level": "debug",
    "account_creation_enabled":true
  },
  "cache":{
    "log_level": "debug",
    "redis":{
      "redis_cache_enabled":true,
      "log_level": "debug",
      "host": "localhost",
      "port": "6379"
    },
    "shared_cache":{
      "shared_cache_enabled":true,
      "flush_cache_on_init":true
    }
  }
}